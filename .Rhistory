f = function(p, soma, n){
res = -n + (soma/p) - (n*exp(1)^(-p)/(1-exp(1)^(-p)))
return(res)
}
f_linha = function(p, soma, n){
res = soma/p^(2) - (-n*exp(1)^(-p)*(1-exp(1)^(-p)) - n*exp(1)^(-2*p)/(1- exp(1)^(-p))^2)
return(res)
}
